<style>
  .top-nav {
    margin: 0 20px;
    margin: 0;
  <% if !$prod %>
    display:inline-flex;
  <% end %>
  <% if heb %>
    zdirection: rtl;
  <% end %>

  }

  .grid-container {
    display: grid;
    margin:5px;
  }

  .footer a {
    margin: 20px;
    /*color: #209CEE;*/
    color: #aab8c2
  }

  .top-nav a:not(.logo) {
    color: #1e5353;
    margin: 20px;
    font-size: 14px;
  }

  .container-fluid, .container-lg, .container-xl, .container-xxl {
    width: 100%;
    padding-right: 0.75rem;
    padding-left: 0.75rem;
    margin-right: auto;
    margin-left: auto;
  }

  .p-2 {
    padding: .5rem!important;
  }
  .justify-content-around {
    justify-content: space-around!important;
  }

  .row {
    --bs-gutter-x: 1.5rem;
    --bs-gutter-y: 0;
    display: flex;
    flex-wrap: wrap;
  }

  .align-items-center {
    align-items: center!important;
  }
  .justify-content-around {
    justify-content: space-around!important;
  }
  .d-flex {
    display: flex!important;
  }

  .top-nav a:not(.logo):hover {
    color: black;
  }

  @media screen and (min-width: 1000px) {
    .logo {
      width: 200px;
      zborder:1px solid red;
      display: inline-block;
      vertical-align: top;
      position: relative;
    }
    .search_go_btn {
      /*display: none;*/
    }

    .top-nav {

      height: 50px;
      /*position: absolute;

      top: 0;
      left: 0;
      z-index: 999;*/
    }

    .top-nav a:not(.logo) {
      background-color: white;
      padding: 5px 10px;
    }
  }


  .search_input {
    padding: 13px 40px 13px 20px;
    border: 1px solid grey;
    font-size: 17px;
    width: 200px;
    margin-left: 0px;
  }
  .links_city_selector_container {
    display: inline-block;
    vertical-align: super;
    margin-bottom: 20px;
  }
  @media screen and (max-width: 800px) {
    .links_city_selector_container {
      zmargin-left: 40px;
    }
    .container {
      margin:10px 0;
      padding:10px 0;
      position: relative;
      /*top: 30px;*/
    }
    .search_input {
      width: 300px;
    }
    .top-nav {
      margin: 10px;

      position: relative;

    }
    .top-nav a, .top-nav .search_input_form {
      display: none;
      margin: 5px;
    }
    /*.fa-gear:hover {
      color: red;
    }*/
    .top-nav a:not(.logo).expanded, .top-nav .search_input_form.expanded {
      display: block;
      padding: 15px;
      background:whitesmoke;
      padding-right: 30px;
      margin-left: 50px;
      color: black;
      text-align: center;
    }

    .top-nav a.logo {
      display: block;
      text-align: center;
    }
    .top-nav a .sub_logo_msg {
      display: block;
      text-align: center;

      width: 100%;
    }
    .hamburger {
      display: block;
    }

    .create_form {
      float: left;
    }

    .search_input {
      padding-right: 0;
    }
  }

  .search_input_form {
    /*  display: inline-block;*/
  }

  .logo_sub_text {
    position: relative;
    top: 20px;
  <% if heb %>
    left: 80px;
  <% else %>
    right: 80px;
  <% end %>
    font-size: 13px;
    color: grey;
  }

  #top_plus_btn {
    font-size: 17px;
    border-radius: 4px;
    padding: 3px 20px;
    position: relative;
    top: 0px;
  }

  .country_icon_container {
    zbackground: linear-gradient(#e66465, #9198e5, #47f53b);
  }

  .country_icon {
    height: 35px;
    width: 35px;
    padding: 0;
    border-radius: 50%;
  }
  .client_logo {
    background-color: #324167;
    padding: 10px;
    border-radius: 10px;
  }
  .sub_logo_msg {
    font-size: 12px;
    color:#1F5151;
    left: 6px;

    top: 27px;
    font-weight: 1000;
  }

  .sign-up-btn{
    border:none;
    font-size:13px;
    margin-left:8rem;
    background:inherit;
    color:#2B6356;
    text-decoration: none;
  }

  .sign-up-btn:hover{
    color:#2B6356;
  }

  .login-btn{
    background:#2B6356;
    color:white;
    border:none;
    font-size:13px;
    border-radius: 20px;
    padding:5px;
    padding-left:30px;
    margin-left: 45rem;
    padding-right:30px;
    text-decoration: none;
  }

  .login-btn:hover
  {
    color:white;
  }

  .container-fluid a{
    text-decoration:none
  }

  @media only screen and (max-width: 1400px){
    .login-btn{
      margin-left: 34rem;
    }
  }

  @media only screen and (max-width: 1200px){
    .login-btn{
      margin-left: 25rem;
    }
  }

  @media only screen and (max-width: 800px){
    .login-btn{
      margin-left: 20rem;
    }
  }

  @media only screen and (max-width: 700px){

    .login-btn{
      margin-left: 10rem;
    }
  }

  @media only screen and (max-width: 700px){
    .login-btn{
      margin-left: 10rem;
    }
  }

  @media only screen and (max-width: 500px){

    .col-sm-7 {
      flex: 0 0 auto;
      width: 58.33333333%;
    }


    .login-btn{
      background:#2B6356;
      color:white;
      border:none;
      font-size:13px;
      border-radius: 20px;
      margin-left: -0.15rem;
      padding:5px;
      padding-left:20px;
      padding-right:20px;
      text-decoration: none;
    }
  }
</style>

<%
  path = _req.path rescue ''
  logo = '/favicon.ico'
  logo = '/img/landing_page/logo6.png'
  use_client_logo = pr[:use_client_logo]
  if use_client_logo
    logo = 'https://images.squarespace-cdn.com/content/v1/5488a9e6e4b0563d5788db1a/12d1da8f-17e9-4462-a4df-d58a3e7c0f27/Tilray+Logo+-+Header.png?format=1500w'
  end
  is_post_page = path.to_s.starts_with?('/posts/') rescue false
  is_main_page = path.to_s == '/' rescue false
%>
<% if path.starts_with?('/admin') %>
  <div style='padding:10px'>

    <a class='btn' href='/'>Homepage</a>
    <a href='/admin'><b>Admin</b></a>
    <a target=_blank class='btn btn-info' href='/admin/admin_stats'>Admin Stats</a>
    <a target=_blank class='btn btn-info' href='/admin/sellers'>Sellers Notes</a>

    <form method=post action="/admin/upload_gdoc_sellers" style='display: inline-block;' enctype="multipart/form-data">
      <label class='btn btn-warning btn-raised'> upload new gdoc of sellers </label>
      <input name=file size=50 type="file" onchange="if (confirm('confirm submission of this file?')) form.submit()" />
    </form>
  </div>
<% else %>

  <div class='container-fluid justify-content-around p-2 '>
    <div class="row">
      <% if path!='/z' %>

        <div class="grid-container col-12 col-sm-7">
          <a class='logo <%=use_client_logo ? 'client_logo' : ''%>' href='/'>
            <img src='<%=logo%>' style='height:30px'/>
            <div class='sub_logo_msg'>discover great cannabis.</div>
          </a>
        </div>

        <% if use_client_logo %>
          <!-- <div>A world of well-being</div> -->
        <% end %>
      <% end %>


      <div class='logo_container' style='display: inline-block; display: none'>
        <a href='/' class='logo'>
          <b style='color: #00b140'>
            <img src='/img/nowcast_logo_pause.png' style='height:80px' />
          </b>
          <small class='logo_sub_text'>
            <%= html_title %>
          </small>
        </a>
      </div>

      <% unless @hide_header %>

        <!-- <a class='nowcast_color' href='/' style='color: #00b140'>
          Good-Weed
        </a>     -->
        <% if cu %>
          <a class='' href='/me'>
            <%= cu_logo %>
            <b><%= cu[:name] %> </b><%= "<b class='nowcast_color'>(pro)</b>" if is_pro(cu) %>
          </a>
          <!-- <a class='' target=_blank href='/@<%=cu['handle']%>'>
        <i class='fa fa-external-link' style='margin-right: 5px'></i> View @<%=cu['handle']%>
      </a>     -->
        <% else %>


          <% if false %>
            <a class='btn btn-raised btn-primary' href='/signup?type=buyer'>
              <!-- https://fontawesome.com/v5.15/icons?d=gallery&p=2&q=dollar -->
              <i class='fal fa-search-dollar' style='margin-right: 10px'></i> <%= !heb ? 'Sign Up To Buy' : 'הרשמה' %>

            </a>
          <% end %>

        <% end %>
        <% if !is_homepage %>
          <a class='' href='/search'><i class='fa fa-search'></i> <%= 'search' %> </a>
        <% end %>
        <% if cu && !is_pro(cu) %>
          <a class='' href='/pro'><i class='fa fa-plus'></i> <%= 'pro' %> </a>
        <% end %>
        <!-- <a class='' href='/about'>about</a>
        <a class='' href='/contact'>contact</a>
        <a class='' href='/articles'>blog</a> -->

        <% if !cu %>
          <div class='col-sm-4 col-12 d-flex align-items-center justify-content-around'>
            <div ><a href="/login" class="login-btn">Login</a></div>

            <div>

              <a href="/signup?type=seller" class="sign-up-btn "><i class='fa fa-user'></i> Sign Up</a>
            </div>
          </div>

        <% end %>
        <!-- <a class='' href='/forum'><i class='fa fa-forum'></i> <%= 'forum' %> </a> -->

        <!-- <a class='' href='/influencers'><i class='fas fa-user-tie' style='margin-right: 5px'></i> <%= 'influencers' %> </a> -->

        <% ([] || ['usa', 'israel']).each do |country| %>
          <a style='padding: 0' class='country_icon_container' href='/?country=<%=country%>'><img class='country_icon' src='/img/icons/<%=country%>.png' /></a>
        <% end %>
        <!-- <a class='' href='/community'>Community</a> -->
        <% if cu && false %>
          <a href='/chat'>

            <i style='zfont-size: 50%; position: relative; zbottom:5px; zmargin-left:20px' class='fal fa-envelope'></i>
            <span class='header_envelope_num_unreads noDisplay'></span>
            <span style='margin-left: 5px'>Messages</span>
          </a>
        <% end %>
        <%#= erb :'search/search_inputs_row' if search_input_on_top %>
        <%
          link = cu ? '/create' : '/signup?type=event'
          text = cu ? '+ create' : 'join'
        %>
        <% if false %>
          <a href='<%= link %>' class='btn btn-primary btn-raised btn-info' id='top_plus_btn'><%= text %></a>
        <% end %>
        <% if cu %>
          <a class='' href='/logout' onclick="return confirm('Are you sure you wish to log out?')"><i class='fal fa-times-circle' ></i></a>
        <% end %>

      <% end %>
      <%= erb :'header/hamburger' %>
    </div>
  </div>
<% end %>